RewriteEngine On    # Turn on the rewriting engine

RewriteRule    ^signin/?$    login.php    [NC,L]   
RewriteRule    ^signup/?$    register.php    [NC,L]   
RewriteRule    ^signup/([A-Za-z0-9-]+)/?$    register.php?ref=$1    [NC,L]   
RewriteRule    ^signout/?$    signout.php    [NC,L]   


RewriteRule    ^ForgotPassword/?$    forgotpass.php    [NC,L]   
RewriteRule    ^dashboard/?$    home.php    [NC,L]   
RewriteRule    ^changepassword/?$    changepass.php    [NC,L]   
RewriteRule    ^updateprofile/?$    updateprofile.php    [NC,L]   


RewriteRule    ^activity/?$    activity.php    [NC,L]   
RewriteRule    ^sendmoney/?$    sendmoney.php    [NC,L]   
RewriteRule    ^requestmoney/?$    requestmoney.php    [NC,L]   


RewriteRule    ^refund/([A-Za-z0-9-]+)/?$    refund.php?id=$1    [NC,L]   
RewriteRule    ^requesttome/?$    requesttome.php    [NC,L]   
RewriteRule    ^requestreject/([A-Za-z0-9-]+)/?$    requestreject.php?id=$1    [NC,L]   
RewriteRule    ^requestsend/([A-Za-z0-9-]+)/?$    requestsend.php?id=$1    [NC,L]   


RewriteRule    ^verifyme/?$    verifyme.php    [NC,L]   



RewriteRule    ^addmoney/?$    addmoney.php    [NC,L]   
RewriteRule    ^add/paypal?$    addpaypal.php    [NC,L]   
RewriteRule    ^add/perfectmoney?$    addpm.php    [NC,L]   
RewriteRule    ^add/agents?$    agents.php    [NC,L]   


RewriteRule    ^verify/?$    verifyphone.php    [NC,L]  



RewriteRule    ^withrawmoney/?$    withrawmoney.php    [NC,L]   
RewriteRule    ^wd/bank?$    wd-bank.php    [NC,L]   
RewriteRule    ^wd/paypal?$    wd-paypal.php    [NC,L] 
RewriteRule    ^wd/perfectmoney?$    wd-perfectmoney.php    [NC,L]   
